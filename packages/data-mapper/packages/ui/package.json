{
  "name": "@datamapper-poc/ui",
  "version": "0.0.1",
  "type": "module",
  "description": "datamapper-poc",
  "repository": "https://github.com/KaotoIO/datamapper-poc",
  "repositoryDirectory": "packages/ui",
  "author": "KaotoIO",
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "types": "./lib/esm/public-api.d.ts",
  "module": "./lib/esm/public-api.js",
  "main": "./lib/cjs/public-api.js",
  "exports": {
    ".": {
      "import": "./lib/esm/public-api.js",
      "require": "./lib/cjs/public-api.js"
    },
    "./testing": {
      "import": "./lib/esm/testing-api.js",
      "require": "./lib/cjs/testing-api.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build --config vite.config.js",
    "build:lib": "yarn rimraf ./lib && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc --project tsconfig.esm.json && copyfiles -u 1 ./src/**/*.scss ./src/assets/** ./lib/esm",
    "build:cjs": "tsc --project tsconfig.cjs.json && copyfiles -u 1 ./src/**/*.scss ./src/assets/** ./lib/cjs",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "yarn eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "yarn lint:code --fix",
    "lint:style": "yarn stylelint \"src/**/*.{css,scss}\"",
    "lint:style:fix": "yarn lint:style --fix"
  },
  "dependencies": {
    "@datamapper-poc/xml-schema-ts": "workspace:*",
    "@dnd-kit/core": "^6.1.0",
    "@patternfly/react-code-editor": "^5.2.3",
    "@patternfly/react-core": "^5.1.2",
    "@patternfly/react-table": "^5.1.2",
    "@patternfly/react-topology": "^5.1.0",
    "@types/pako": "^2.0.3",
    "chevrotain": "10.5.0",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "file-saver": "^2.0.5",
    "loglevel": "^1.9.1",
    "monaco-editor": "^0.51.0",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-use-gesture": "^9.1.3",
    "use-debounce": "^10.0.0",
    "xml-formatter": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@storybook/addon-actions": "^8.0.0",
    "@storybook/addon-knobs": "^8.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.6",
    "@types/react": "^18.2.53",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "madge": "^8.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.70.0",
    "stylelint": "^16.2.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-prettier": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-circular-dependency": "^0.4.0",
    "xslt-ts": "^1.1.8"
  }
}
